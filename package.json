{
  "name": "best-offers",
  "version": "1.0.0",
  "description": "Application for searching flights",
  "author": "harshit t",
  "license": "ISC",
  "scripts": {
    "server": "cd server && npm start",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test:unit": "vitest --coverage --ui",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "rl": "npm-run-all -p server dev"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "date-fns": "^2.30.0",
    "open-sans-fontface": "*",
    "pinia": "^2.0.23",
    "sass": "^1.63.3",
    "vue": "^3.2.0",
    "vue-router": "^4.2.2",
    "vuetify": "npm:@vuetify/nightly@3.3.3-pr-17265.321510f",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.0",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/browser": "^0.32.0",
    "@vitest/coverage-istanbul": "^0.32.0",
    "@vitest/ui": "^0.32.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.0",
    "vite": "^4.2.0",
    "vite-plugin-vuetify": "^1.0.0",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.2.0",
    "webdriverio": "^8.11.2",
    "npm-run-all": "^4.1.5"
  }
}
